{
	"name": "barefoot-fire",
	"version": "1.0.0",
	"description": "A financial dashboard using PocketSmith for Obsidian",
	"main": "main.js",
	"keywords": [],
	"author": "Serge Krstic",
	"license": "MIT",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"format": "prettier --write .",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
